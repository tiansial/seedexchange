$('.like_seed').bind('ajax:success', function(){
   $(this).parent().find('.likecount-<%= @seed.id %>').html('<%= escape_javascript @seed.votes_for.size.to_s %>');
   $(this).closest('.like_seed').hide();
   $(this).closest('.likes').html(' <%= link_to "Unlike", unlike_seed_path(@seed), remote: true, method: :get, class: 'unlike_seed' %>');
});