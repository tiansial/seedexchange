$('.unlike_seed').bind('ajax:success', function(){
   $(this).parent().find('.likecount-<%= @seed.id %>').html('<%= escape_javascript @seed.votes_for.size.to_s %>');
   $(this).closest('.unlike_seed').hide();
   $(this).closest('.likes').html(' <%= link_to "Like", like_seed_path(@seed), remote: true, method: :get, class: 'like_seed' %>');

});